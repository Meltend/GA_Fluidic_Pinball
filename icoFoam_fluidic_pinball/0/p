FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    cylinder_a
    {
        type            zeroGradient;
    }
	cylinder_b
    {
        type            zeroGradient;
    }
	cylinder_c
    {
        type            zeroGradient;
    }
	outlet
    {
        type            fixedValue;
		value			uniform 0;
    }
	
    front
    {
        type            empty;
    }
	back
    {
        type            empty;
    }
}


